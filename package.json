{
  "name": "blue-herring",
  "version": "0.0.1",
  "description": "Automatic form generation for React projects",
  "main": "build/blue-herring.js",
  "typings": "build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "test": "jest --env=jsdom",
    "compile": "tonnelier src > src/index.ts && webpack"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/implydata/blue-herring.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/implydata/blue-herring/issues"
  },
  "homepage": "https://github.com/implydata/blue-herring#readme",
  "dependencies": {
    "@blueprintjs/core": "^3.14.1",
    "classnames": "^2.2.6",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.0",
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-test-renderer": "^16.8.1",
    "@types/user-event": "^1.4.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.0",
    "enzyme-to-json": "^3.3.5",
    "hear-ye": "0.0.28",
    "identity-obj-proxy": "^3.0.0",
    "import-glob-loader": "^1.1.0",
    "jest": "^24.5.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^6.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tonnelier": "0.0.3",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "user-event": "^1.4.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3"
  }
}
